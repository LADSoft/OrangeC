%.exe: %.c
	occ  /! /D__ILP32__ /D__noinline__=__stdcall__ $^

%.tst: %.exe
	$^ > $*.tst
	fc $*.tst $*.c.expected

all: $(FILES:.exe=.tst)

clean:
	$(CLEAN)